{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    {
     "route": "/teste",
     "rewrite": "/teste.html"
    }
   ],
   "responseOverrides": {
    "400": {
      "redirect": "/login",
      "statusCode": 301
    },
    "401": {
      "rewrite": "/error.html"
    },
    "403": {
      "rewrite": "/error.html"
    },
    "404": {
      "rewrite": "/error.html"
    }
  }
}
